<h1>加成A</h1>

<%= form_for @plusa , :url => updateplusratea_yfcase_objectbuild_path( @yfcase,@plusa), method: :patch do |f| %>
<div class="form-group">
    <%= f.label :plusa %>
    <%= f.number_field :plusa, in: 0..10, :step => 0.01, :required => true , :class => "form-control" %>
    <%= f.hidden_field :surveyora, :value => "#{current_user.first_name}#{current_user.last_name}" %>
</div>

<div class="form-group">
    <%= f.submit :class => "btn btn-primary" %>
</div>
<% end %>
<%= link_to "back", yfcase_path(@yfcase) %>
