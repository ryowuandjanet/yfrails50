			<td style="border:1px solid black;color:blue;" rowspan="2">
				<%= @yfcase.personnals.where(is_original_owner: true).blank? ? ' ' :  @yfcase.personnals.where(is_original_owner: true).first.right_share_person %>/<%= @yfcase.personnals.where(is_original_owner: true).first.right_share_all %>
			</td>
			<td style="border:1px solid black;" rowspan="12">一般案件</td>

