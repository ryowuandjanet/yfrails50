			<td style="border:1px solid black;width:12px;">1</td>
			<td style="border:1px solid black;width:13px;">0</td>
			<td style="border:1px solid black;width:13px;">9</td>
			<td style="border:1px solid black;width:13px;">0</td>
			<td style="border:1px solid black;width:13px;">4</td>
			<td style="border:1px solid black;width:13px;">1</td>
			<td style="border:1px solid black;width:13px;">7</td>
